<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
     <!ENTITY rec  "http://uff.ic.br/ontologias/recomendador/" >
     <!ENTITY owl  "http://www.w3.org/2002/07/owl#" >
     <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
     <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
     <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema#" >
   ]>

<rdf:RDF
  xmlns     = "http://uff.ic.br/ontologias/recomendador/"
  xml:base  = "http://uff.ic.br/ontologias/recomendador/"
  xmlns:owl = "http://www.w3.org/2002/07/owl#"
  xmlns:rdf = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs= "http://www.w3.org/2000/01/rdf-schema#"
  xmlns:xsd = "http://www.w3.org/2001/XMLSchema#">

  <owl:Ontology rdf:about="">
    <rdfs:label>Wine and Food Recommendation Ontology</rdfs:label>
    <rdfs:comment>Unified ontology for wine-food pairing recommendations following OWL2 best practices</rdfs:comment>
    <owl:versionInfo rdf:datatype="&xsd;string">2.0.0</owl:versionInfo>
    <rdfs:seeAlso rdf:resource="https://github.com/seu-usuario/wine-recommender"/>
  </owl:Ontology>

</rdf:RDF>
